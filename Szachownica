size(80, 80); 
background(255); 
stroke(255);
noFill();
smooth(); 





fill (0);

rect(0, 0, 10, 10);

for (int i = 10; i < 80; i = i+20) {
 for (int j = 10; j < 80; j = j+20) {
 rect(j, i, 10, 10);
}
}

rect(10, 10, 10, 10);

for (int i = 0; i < 80; i = i+20) {
 for (int j = 0; j < 80; j = j+20) {
 rect(j, i, 10, 10);
}
}
